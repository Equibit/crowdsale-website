{
  "name": "equibit-crowdsale",
  "version": "0.0.0",
  "description": "An awesome DoneJS app",
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MarcGodard/equibit-crowdsale-website.git"
  },
  "author": {
    "name": "",
    "email": "",
    "url": ""
  },
  "private": true,
  "scripts": {
    "build": "node build",
    "develop": "done-serve --static --develop --port 8080",
    "lint": "standard",
    "lint:fix": "standard --fix",
    "start": "done-serve --port 8080",
    "test": "npm run lint && npm run testee",
    "testee": "testee test.html --browsers firefox --reporter Spec"
  },
  "main": "equibit-crowdsale/index.stache!done-autorender/no-zone",
  "files": [
    "src"
  ],
  "keywords": [
    "",
    "donejs-app"
  ],
  "steal": {
    "main": "equibit-crowdsale/index.stache!done-autorender/no-zone",
    "directories": {
      "lib": "src"
    },
    "configDependencies": [
      "live-reload",
      "node_modules/can-zone/register"
    ],
    "plugins": [
      "done-css",
      "done-component",
      "steal-less",
      "steal-stache"
    ],
    "envs": {
      "server-production": {
        "renderingBaseURL": "/dist"
      }
    },
    "serviceBaseURL": "http://localhost:3030"
  },
  "standard": {
    "env": [
      "mocha"
    ]
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "bootstrap-select": "^1.12.4",
    "can-component": "^3.3.4",
    "can-connect": "^1.5.8",
    "can-connect-feathers": "^3.6.1",
    "can-define": "^1.5.3",
    "can-route": "^3.2.3",
    "can-route-pushstate": "^3.1.1",
    "can-set": "^1.3.2",
    "can-stache": "^3.7.2",
    "can-view-autorender": "^3.1.1",
    "can-view-callbacks": "^3.2.0",
    "can-zone": "^0.6.10",
    "done-autorender": "^1.3.1",
    "done-component": "^1.0.0",
    "done-css": "^3.0.1",
    "done-serve": "^1.4.0",
    "donejs-feathers": "^2.1.5",
    "eonasdan-bootstrap-datetimepicker": "^4.17.47",
    "feathers": "^2.2.0",
    "feathers-authentication-client": "^0.3.3",
    "feathers-hooks": "^2.0.2",
    "feathers-rest": "^1.8.0",
    "feathers-socketio": "^2.0.0",
    "font-awesome": "^4.7.0",
    "generator-donejs": "^1.0.7",
    "grid-component": "^0.10.2",
    "jquery": "^3.2.1",
    "moment": "^2.18.1",
    "socket.io-client": "^2.0.3",
    "steal": "^1.5.13",
    "steal-less": "^1.2.0",
    "steal-socket.io": "^4.0.9",
    "steal-stache": "^3.1.1",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "can-fixture": "^1.1.1",
    "chai": "^4.1.2",
    "donejs-cli": "^1.0.0",
    "funcunit": "^3.4.3",
    "standard": "^10.0.3",
    "steal-mocha": "^1.0.0",
    "steal-tools": "^1.2.0",
    "testee": "^0.7.0"
  }
}
