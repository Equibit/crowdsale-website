{
  "name": "easy-app",
  "version": "0.0.0",
  "description": "An awesome DoneJS app",
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MarcGodard/easy-app.git"
  },
  "author": {
    "name": "",
    "email": "",
    "url": ""
  },
  "private": true,
  "scripts": {
    "test": "testee test.html --browsers firefox --reporter Spec",
    "start": "done-serve --port 8080",
    "develop": "done-serve --develop --port 8080",
    "build": "node build"
  },
  "main": "easy-app/index.stache!done-autorender",
  "files": [
    "src"
  ],
  "keywords": [
    "",
    "donejs-app"
  ],
  "steal": {
    "main": "easy-app/index.stache!done-autorender",
    "directories": {
      "lib": "src"
    },
    "configDependencies": [
      "live-reload",
      "node_modules/can-zone/register"
    ],
    "plugins": [
      "done-css",
      "done-component",
      "steal-less",
      "steal-stache"
    ],
    "envs": {
      "server-production": {
        "renderingBaseURL": "/dist"
      }
    },
    "serviceBaseURL": "https://easyappframework.com/api"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "bootstrap-select": "^1.12.4",
    "can-component": "^3.0.7",
    "can-connect": "^1.5.8",
    "can-connect-feathers": "^3.6.1",
    "can-define": "^1.4.2",
    "can-route": "^3.2.1",
    "can-route-pushstate": "^3.0.3",
    "can-set": "^1.3.2",
    "can-stache": "^3.6.1",
    "can-view-autorender": "^3.1.1",
    "can-view-callbacks": "^3.2.0",
    "can-zone": "^0.6.6",
    "done-autorender": "^1.0.0",
    "done-component": "^1.0.0",
    "done-css": "^3.0.0",
    "done-serve": "^1.0.0",
    "donejs-feathers": "^2.1.5",
    "eonasdan-bootstrap-datetimepicker": "^4.17.47",
    "feathers": "^2.1.7",
    "feathers-authentication-client": "^0.3.3",
    "feathers-hooks": "^2.0.2",
    "feathers-rest": "^1.8.0",
    "feathers-socketio": "^2.0.0",
    "font-awesome": "^4.7.0",
    "fullpage.js": "^2.9.4",
    "generator-donejs": "^1.0.0",
    "grid-component": "^0.9.0",
    "jquery": "^3.2.1",
    "moment": "^2.18.1",
    "steal": "^1.3.1",
    "steal-less": "^1.2.0",
    "steal-socket.io": "^4.0.9",
    "steal-stache": "^3.0.5",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "can-fixture": "^1.0.13",
    "donejs-cli": "^1.0.0",
    "funcunit": "^3.2.0",
    "steal-qunit": "^1.0.1",
    "steal-tools": "^1.2.0",
    "testee": "^0.3.0"
  }
